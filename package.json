{
  "name": "franz",
  "version": "1.0.0",
  "description": "Kafka Manager",
  "main": "index.js",
  "sideEffects": false,
  "types": "types/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run prod",
    "build": "cross-env NODE_ENV=production webpack",
    "prod": "cross-env NODE_ENV=production ts-node src/server/server.ts",
    "prepare": "husky install",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack serve --open\" \"cross-env NODE_ENV=development nodemon src/server/server.ts\"",
    "test": "jest",
    "windev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --open --hot\" \"nodemon src/server/server.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/franz.git"
  },
  "author": "Rachelle Maiorca, Ryan Hastie, Chris Dunleavy, Patrick Reid, Jonathan Haviv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/franz/issues"
  },
  "homepage": "https://github.com/oslabs-beta/franz#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
